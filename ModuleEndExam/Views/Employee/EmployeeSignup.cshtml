@{
    ViewData["Title"] = "Home Page";
}
<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" integrity="sha384-pzjw8k+TqDRTTFKv+l/Cr5qfPxNL1ic+E99o8gd5MwwU5fBWf4UfGJw6TA0Dw4oJ" crossorigin="anonymous">
<h1>Welcome to College</h1>
<br />
<p>This is the Signup page.</p>

<h2>Employee Signup</h2>
<form method="post" action="/Employee/Signup">
    <div class="form-group">
        <label for="employeeName">Name:</label>
        <input type="text" id="employeeName" name="employeeName" class="form-control" required />
    </div>
    <br />
    <div class="form-group">
        <label for="employeeEmail">Email:</label>
        <input type="email" id="employeeEmail" name="employeeEmail" class="form-control" required />
    </div>
    <br />
    <div class="form-group">
        <label for="employeePassword">Password:</label>
        <input type="password" id="employeePassword" name="employeePassword" class="form-control" required />
    </div>
    <br />
    <div class="form-group">
        <button type="submit" class="btn btn-primary">Signup</button>
    </div>
</form>